{
    "name": "sebastiansulinski/php-paginator",
    "description": "Framework agnostic PHP Pagination component",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Sulinski",
            "email": "info@ssdtutorials.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/http": "^11.0",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "SSD\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
